<p id="notice"><%= notice %></p>

<p>
  <strong>Body:</strong>
  <%= @question.body %>
</p>

<p>
  <strong>Topic:</strong>
  <%= @question.topic %>
</p>

<p>
  <strong>Level:</strong>
  <%= @question.level %>
</p>

<p>
  <strong>Exam:</strong>
  <%= @question.exam_id %>
</p>

<p>
  <strong>Options:</strong>
  <% @question.options.each do |o| %>
    <p><%= o.text %></p>
  <% end %> 
</p>

<%= link_to 'Edit', edit_question_path(@question) %> |
<%= link_to 'Back', questions_path %>
