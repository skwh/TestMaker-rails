<div class="page-header">
  <h1>
    <%= @test.title %>
    <%= link_to 'Edit', edit_test_path(@test), class:"btn btn-xs btn-info" %>
  </h1>
</div>
<div class="well">
  <%= @test.description %>
</div>
<div class="page-header">
  <h2>Questions</h2>
</div>
<div class="list-group">
  <% @test.questions.each do |q| %>
    <a href="#" class="list-group-item">
      <h4 class="list-group-item-heading"><%= q.text %></h4>
      <p class="list-group-item-text"><%= q.level %></p>
      <p class="list-group-item-text"><%= q.type %></p>
    </a>
  <% end %>
</div>
